<%- include('partials/header'); -%>


<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input type="text" name="postTitle" class="form-control">
  </div>
  <div class="form-group">
    <label for="post">Post</label>
    <textarea name="postBody" rows="5" cols="30" id="post" class="form-control"></textarea>
  </div>
  <button type="submit" name="button" class="btn btn-primary">Publish</button>

</form>

<%- include('partials/footer'); -%>
